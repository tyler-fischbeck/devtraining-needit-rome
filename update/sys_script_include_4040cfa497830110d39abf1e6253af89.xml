        <api_name>x_58872_needit.validateEmail</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>On demand Script Include to validate email address syntax using regular expressions.</description>
        <name>validateEmail</name>
        <script><![CDATA[function validateEmail(emailString) {
    email = emailString + '';

    if (email.match(/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/)) {
        return true;
    } else {
        return false;
    }
}]]></script>
